					<set id="row1" appearance="whitespace" parent="body" />
					<set id="row2" appearance="whitespace" parent="body"
						template="/org/alfresco/components/form/2-column-set.ftl" />
					<set id="vigencia" appearance="bordered-panel" label-id="workflow.tecnowf_conformidad.vigencia"
						parent="body" template="/org/alfresco/components/form/2-column-set.ftl" />
					<set id="row3" appearance="whitespace" parent="body" />
