					<set id="row5" appearance="whitespace" parent="body"
						template="/org/alfresco/components/form/2-column-set.ftl" />
					<set id="row6" appearance="whitespace" parent="body" />
					<set id="row7" appearance="whitespace" parent="body"
						template="/org/alfresco/components/form/2-column-set.ftl" />
					<set id="row8" appearance="whitespace" parent="body" />

					<field id="tecno:areaResponsable" set="row1" />
					<field id="tecno:direccion" set="row1">
						<control template="/afip/tecno/form/controls/selectone.ftl" />
					</field>
					<field id="tecno:domicilio" set="row1" />

					<field id="tecno:expediente" set="row2">
						<control>
							<control-param name="styleClass">short-field</control-param>
						</control>
					</field>
					<field id="tecno:ordenCompra" set="row2" />

					<field id="tecno:fechaInicio" set="vigencia" />
					<field id="tecno:fechaVencimiento" set="vigencia" />

					<field id="tecno:objetoServicio" set="row3">
						<control template="/afip/tecno/form/controls/textarea.ftl">
							<control-param name="style">width: 95%</control-param>
						</control>
					</field>
					<field id="tecno:tipoServicio" set="row3" />
					<field id="tecno:otroTipoServicio" set="row3" />
					<field id="tecno:periodicidad" set="row3" />

					<field id="tecno:periodoDesde" set="periodo" />
					<field id="tecno:periodoHasta" set="periodo" />

					<field id="tecno:proveedor" set="row4" />
					<field id="tecno:cuitProveedor" set="row4" />

					<field id="tecno:remitoNumero" set="row5" />
					<field id="tecno:facturaNumero" set="row5" />

					<field id="tecno:regimenContratacion" set="row6" />

					<field id="tecno:notaCredito" set="row7" />
					<field id="tecno:notaDebito" set="row7" />

					<field id="tecno:items" set="row8">
						<control template="/org/alfresco/form/controls/datatable-property.ftl">
							<control-param name="config"><![CDATA[
								nroRenglon, N° Renglon, number; 
								item, Item, number;
								cantServiciosOC, Cant Servicios O/C, number;
								nroServicioTotOC, N° Servicio, number;
								cantidad, Cantidad , number;
								precioUnitario, Precio Unitario, currency;
								precioTotal, Precio Total, currency; 
							]]></control-param>
							<control-param name="debug">false</control-param>
						</control>
					</field>
					<field id="packageItems" set="row8" />
					<field id="transitions" set="row8" />
				</appearance>
			</form>
		</forms>
	</config>

