<alfresco-config>
	<config>
		<forms>
			<default-controls>
				<type name="text" template="/afip/tecno/form/controls/textfield.ftl" />
				<type name="textarea" template="/afip/tecno/form/controls/textarea.ftl" />
				<type name="int" template="/afip/tecno/form/controls/number.ftl" />
				<type name="float" template="/afip/tecno/form/controls/number.ftl" />
				<type name="double" template="/afip/tecno/form/controls/number.ftl" />
				<type name="long" template="/afip/tecno/form/controls/number.ftl" />
				<type name="date" template="/afip/tecno/form/controls/date.ftl" />
			</default-controls>
			<dependencies>
				<css src="/components/tecno/css/customform.css" />
			</dependencies>
		</forms>
	</config>

	<config evaluator="string-compare" condition="activiti$conformidad">
		<forms>
			<form>
				<field-visibility>
					<show id="bpm:workflowDescription" />
					<show id="packageItems" />
					<show id="transitions" />
				</field-visibility>
				<appearance>
					<set id="title" appearance="title" label-id="workflow.set.general" />
					<set id="items" appearance="title" label-id="workflow.set.items" />

					<field id="transitions" label-id="transitions.label" set="title" />
					<field id="bpm:workflowDescription" label-id="workflow.field.message"
						set="title">
						<control template="/afip/tecno/form/controls/textarea.ftl">
							<control-param name="style">width: 95%</control-param>
						</control>
					</field>
					<field id="packageItems" set="items" />
				</appearance>
			</form>
		</forms>
	</config>

	<config evaluator="task-type" condition="tecnowf:form1900">
